{
  "name": "beastbytes/yii-tracy-panel-user",
  "description": "User panel for Yii Tracy",
  "keywords": [
    "yii",
    "Tracy",
    "debugger",
    "user"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.1",
    "beastbytes/yii-tracy": "dev-main",
    "yiisoft/rbac": "^2.0",
    "yiisoft/user": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Yii\\Tracy\\Panel\\User\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeastBytes\\Yii\\Tracy\\Panel\\User\\Tests\\": "tests/"
    }
  },
  "extra": {
    "config-plugin-options": {
      "source-directory": "config"
    },
    "config-plugin": {
      "params-web": "params-web.php"
    }
  }
}
